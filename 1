G = int(input("Enter the Marks "))

if 0<= G :
    if 80< G :
            print("Your Grade is ‘A’")
    elif 60< G <=80 :
            print("Your Grade is ‘B’")
    elif 50< G <=60 :
            print("Your Grade is ‘C’")
    elif 45< G <=50 :
            print("Your Grade is ‘D’")
    elif 25<= G <=45 :
            print("Your Grade is ‘E’")
    elif 0<= G <=25 :
            print("Your Grade is ‘F’")
    else:
        pass
else:
     print("Enter a Valid Marks")